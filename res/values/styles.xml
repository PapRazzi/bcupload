<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="UploadLabels">
        <item name="android:textSize">16dip</item>
        <item name="android:textColor">#0396E3</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TitleBarBackground">
        <item name="android:background">#0396E3</item>
    </style>

    <style name="TitleBar" parent="android:TextAppearance.WindowTitle">
        <item name="android:textSize">20dip</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:background">#0396E3</item>
    </style>

    <style name="GSOITheme" parent="android:Theme">
        <item name="android:windowTitleSize">50dip</item>
        <item name="android:windowTitleStyle">@style/TitleBar</item>
        <item name="android:windowTitleBackgroundStyle">@style/TitleBarBackground</item>
        <item name="android:background">#FFFFFF</item>
    </style>

    <style name="Button" parent="@android:style/Widget.Button">
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">18dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">#0396E3</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

</resources>